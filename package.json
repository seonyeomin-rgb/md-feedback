{
  "name": "md-feedback",
  "displayName": "MD Feedback",
  "description": "Review markdown plans visually, checkpoint your progress, and export structured handoff documents to Claude Code, Cursor, or any AI coding agent. Includes MCP server for direct AI integration.",
  "version": "0.6.0",
  "publisher": "yeominux",
  "license": "MIT",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e293b",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seonyeomin-rgb/md-feedback"
  },
  "homepage": "https://github.com/seonyeomin-rgb/md-feedback#readme",
  "bugs": {
    "url": "https://github.com/seonyeomin-rgb/md-feedback/issues"
  },
  "qna": "marketplace",
  "markdown": "github",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "markdown",
    "review",
    "feedback",
    "annotation",
    "ai",
    "plan",
    "context",
    "export",
    "claude",
    "cursor",
    "vibe-coding",
    "code-review",
    "handoff",
    "structured-feedback",
    "ai-agent",
    "markdown-review",
    "plan-review",
    "ai-context",
    "coding-workflow",
    "developer-tools",
    "productivity",
    "quality",
    "markdown-annotation",
    "visual-review",
    "ai-coding"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "md-feedback",
          "title": "MD Feedback",
          "icon": "icon.svg"
        }
      ]
    },
    "views": {
      "md-feedback": [
        {
          "type": "webview",
          "id": "md-feedback.panel",
          "name": "Review"
        }
      ]
    },
    "commands": [
      {
        "command": "md-feedback.exportClaude",
        "title": "Export: Claude Code (CLAUDE.md)",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportCursor",
        "title": "Export: Cursor (.cursor/rules/)",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportCodex",
        "title": "Export: Codex (AGENTS.md)",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportCopilot",
        "title": "Export: GitHub Copilot",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportCline",
        "title": "Export: Cline (.clinerules)",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportWindsurf",
        "title": "Export: Windsurf (.windsurfrules)",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportRooCode",
        "title": "Export: Roo Code (.roo/rules/)",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportGemini",
        "title": "Export: Gemini (.gemini/styleguide.md)",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportAntigravity",
        "title": "Export: Antigravity (.agent/rules/)",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportGeneric",
        "title": "Export: Generic Markdown",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportAll",
        "title": "Export: All Tools (write all context files)",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.checkpoint",
        "title": "Create Checkpoint",
        "category": "MD Feedback"
      },
      {
        "command": "md-feedback.exportHandoff",
        "title": "Export: Handoff Document",
        "category": "MD Feedback"
      }
    ]
  },
  "sponsor": {
    "url": "https://buymeacoffee.com/ymnseon8"
  },
  "bin": {
    "md-feedback-mcp": "./dist/mcp-server.js"
  },
  "scripts": {
    "build": "npm run build:ext && npm run build:webview && npm run build:mcp",
    "build:ext": "node esbuild.mjs",
    "build:webview": "vite build",
    "build:mcp": "node mcp/esbuild.mjs",
    "watch:ext": "node esbuild.mjs --watch",
    "dev": "concurrently \"npm run watch:ext\" \"vite build --watch\"",
    "package": "npx @vscode/vsce package --no-dependencies",
    "sync": "node scripts/sync-shared.mjs"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@rollup/rollup-linux-x64-gnu": "^4.57.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@tiptap/core": "^2.11.5",
    "@tiptap/extension-bubble-menu": "^2.11.5",
    "@tiptap/extension-highlight": "^2.11.5",
    "@tiptap/extension-image": "^2.27.2",
    "@tiptap/extension-link": "^2.27.2",
    "@tiptap/extension-placeholder": "^2.11.5",
    "@tiptap/extension-table": "^2.27.2",
    "@tiptap/extension-table-cell": "^2.27.2",
    "@tiptap/extension-table-header": "^2.27.2",
    "@tiptap/extension-table-row": "^2.27.2",
    "@tiptap/extension-task-item": "^2.27.2",
    "@tiptap/extension-task-list": "^2.27.2",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/react": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/vscode": "^1.85.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vscode/vsce": "^3.7.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "esbuild": "^0.24.0",
    "nanoid": "^5.1.2",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.17",
    "tiptap-markdown": "^0.8.10",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
